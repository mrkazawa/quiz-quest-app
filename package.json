{
  "name": "quiz-quest",
  "version": "1.0.0",
  "description": "A simple multiple choice quiz application like Kahoot",
  "main": "api/server.js",
  "scripts": {
    "start": "node api/server.js",
    "dev": "nodemon api/server.js",
    "dev:client": "cd app-react && npm run dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:client\"",
    "build:client": "cd app-react && npm run build",
    "install:client": "cd app-react && npm install"
  },
  "keywords": [
    "quiz",
    "kahoot",
    "realtime",
    "socket.io"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^4.18.2",
    "express-session": "^1.18.1",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "nodemon": "^3.0.1"
  }
}
