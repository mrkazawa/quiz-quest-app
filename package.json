{
  "name": "quiz-quest",
  "version": "1.0.0",
  "description": "A simple multiple choice quiz application like Kahoot",
  "private": true,
  "scripts": {
    "start": "cd api-ts && npm start",
    "build": "npm run build:all",
    "build:all": "npm run build:api && npm run build:client",
    "build:api": "cd api-ts && npm run build",
    "build:client": "cd client && npm run build",
    "dev": "npm run dev:all",
    "dev:all": "node start-dev-servers.js",
    "dev:api": "cd api-ts && npm run dev",
    "dev:client": "cd client && npm run dev"
  },
  "keywords": [
    "quiz",
    "kahoot",
    "realtime",
    "socket.io"
  ],
  "author": "",
  "license": "ISC"
}